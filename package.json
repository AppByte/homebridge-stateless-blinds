  "devDependencies": {
    "@types/node": "~16.10.9",
    "@typescript-eslint/eslint-plugin": "~5.0.0",
    "@typescript-eslint/parser": "~5.0.0",
    "eslint": "~8.0.1",
    "homebridge": "~1.3.5",
    "nodemon": "~2.0.13",
    "rimraf": "~3.0.2",
    "ts-node": "~10.3.0",
    "typescript": "~4.4.4"
  }
}
